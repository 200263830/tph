        <script src="https://sdk.canva.com/partnership.js" src_type="url"/>
        <css src="Tph_Onlinedesign::css/canva.css"/>
    </head>
   <referenceContainer name="content">
        <block class="Tph\Onlinedesign\Block\Canvapage" name="onlinedesign" template="Tph_Onlinedesign::index.phtml"/>
    </referenceContainer>
    <referenceBlock name="navigation.sections" remove="true" />
    <referenceBlock name="catalog.compare.sidebar" remove="true"/>
    <referenceBlock name="page.main.title" remove="true"/>
    <referenceBlock name="wishlist_sidebar" remove="true" />
    <referenceContainer name="footer" remove="true"/>
    <referenceContainer name="form.subscribe" remove="true"/>
    <referenceContainer name="copyright" remove="true"/>
